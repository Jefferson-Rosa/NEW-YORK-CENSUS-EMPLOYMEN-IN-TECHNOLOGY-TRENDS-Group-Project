  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Demography</DataSourceName>
        <CommandText>SELECT        FactDemography.Total_Distribution, DimDeptType.Dept_name
FROM            FactDemography INNER JOIN
                         DimDeptType ON FactDemography.DeptKey = DimDeptType.DeptKey
WHERE        (DimDeptType.Dept_name = N'Non-tech')
ORDER BY FactDemography.Total_Distribution</CommandText>
      </Query>
